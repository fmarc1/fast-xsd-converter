{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "Root": {
      "type": "object",
      "properties": {
        "codes": {
          "$ref": "#/$defs/CodeList"
        }
      },
      "required": ["codes"]
    }
  },
  "required": ["Root"],
  "$defs": {
    "CodeList": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^(?:[A-Z]{2}[0-9]{2})$"
      }
    }
  }
}
