{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "Root": {
      "$ref": "#/$defs/Container"
    }
  },
  "required": ["Root"],
  "$defs": {
    "Container": {
      "type": "object",
      "properties": {
        "item": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "value": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        }
      },
      "required": ["item", "value"]
    }
  }
}
