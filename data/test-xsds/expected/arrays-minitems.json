{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "Container": {
      "type": "object",
      "properties": {
        "entry": {
          "type": "array",
          "minItems": 2,
          "items": {
            "type": "string"
          }
        },
        "block": {
          "type": "array",
          "minItems": 3,
          "items": {
            "type": "object",
            "properties": {
              "value": {
                "type": "integer"
              }
            },
            "required": ["value"]
          }
        },
        "note": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["entry", "block"]
    }
  },
  "required": ["Container"],
  "$defs": {}
}
