{
	"name": "fast-xsd-converter",
	"version": "0.1.3",
	"description": "Convert XML Schema (XSD) to JSON Schema.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fmarc1/fast-xsd-converter.git"
	},
	"bugs": {
		"url": "https://github.com/fmarc1/fast-xsd-converter/issues"
	},
	"homepage": "https://github.com/fmarc1/fast-xsd-converter#readme",
	"keywords": ["xsd", "xml", "json-schema", "schema", "converter"],
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": ["dist", "README.md", "CHANGELOG.md", "LICENSE"],
	"sideEffects": false,
	"scripts": {
		"build": "bun x tsc -p tsconfig.build.json",
		"test": "bun test",
		"bench": "bun run bench/bench.ts",
		"release": "bun run scripts/release.ts"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"ajv": "^8.17.1",
		"prettier": "^3.8.0",
		"typescript": "^5",
		"xsd2jsonschema": "^0.3.7"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"fast-xml-parser": "^5.3.3"
	}
}
